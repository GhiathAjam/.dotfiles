{
  "configurations": {
    "Launch": {
      "adapter": "vscode-cpptools",
      "filetypes": [ "cpp", "c", "objc" ],
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/_main",
        "args": ["< input.txt"],
        "cwd": "${workspaceRoot}",
        "MIMode": "gdb"
      }
    }
  }
}
